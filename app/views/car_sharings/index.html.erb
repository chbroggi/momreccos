<p id="notice"><%= notice %></p>

<h1>Car Sharings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Nb seats</th>
      <th>Event</th>
      <th>Player</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_sharings.each do |car_sharing| %>
      <tr>
        <td><%= car_sharing.name %></td>
        <td><%= car_sharing.description %></td>
        <td><%= car_sharing.nb_seats %></td>
        <td><%= car_sharing.event %></td>
        <td><%= car_sharing.player %></td>
        <td><%= link_to 'Show', car_sharing %></td>
        <td><%= link_to 'Edit', edit_car_sharing_path(car_sharing) %></td>
        <td><%= link_to 'Destroy', car_sharing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car Sharing', new_car_sharing_path %>
